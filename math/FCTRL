#include<bits/stdc++.h>
#define fr(i,c,b) for(int i=c;i<b;i++)
#define si(x) int x; scanf("%d", &x)
#define sll(x) ll x; scanf("%lld", &x)
#define pi(x) printf("%d", x)
#define pll(x) printf("%lld", x)
#define nl printf("\n")
#define watch(x) cout << (#x) << " is " << (x) << endl

typedef long long int ll;

using namespace std;

int main()
{
    si(t);
    while(t--)
    {
        sll(n);
        ll ans=0;
        
        while(n>0)
        {
            n/=5;
            ans+=n;
        }
        cout<<ans<<endl;
    }
}
