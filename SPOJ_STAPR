//https://www.spoj.com/problems/STPAR

#include<bits/stdc++.h>
#define fr(i,c,b) for(int i=c;i<b;i++)
#define si(x) int x; scanf("%d", &x)
#define sll(x) ll x; scanf("%lld", &x)
#define pi(x) printf("%d", x)
#define pll(x) printf("%lld", x)
#define nl printf("\n")
#define watch(x) cout << (#x) << " is " << (x) << endl

typedef long long int ll;

using namespace std;

int main()
{
    si(t);
    while(t!=0)
    {
        ll n=t;
        ll a[n];
        fr(i,0,n)
            cin>>a[i];
        stack<int> s,side;
        int j=1;
        for(int i=0;i<n;i++)
        {
            if(s.empty()&&a[i]==j)
                s.push(a[i]), j++;
            else if(s.empty()&&a[i]!=j)
                side.push(a[i]);
            else
                {
                if(a[i]==j)
                {
                    s.push(a[i]);
                    j++;
                }
                else if(!side.empty()&&side.top()==j)
                {
                    while(!side.empty()&&side.top()==j)
                    {s.push(side.top());
                    side.pop();
                    j++;
                    }
                    side.push(a[i]);
                }
                else if(side.empty())
                {
                    side.push(a[i]);
                }
                else if(!side.empty()&&side.top()!=j)
                {
                    side.push(a[i]);
                }
        }}
        while(!side.empty())
        {
            s.push(side.top());
            side.pop();
        }
        fr(i,0,n)
        {
            if(!s.empty())
            {
                a[i]=s.top();
                s.pop();
            }
        }
        int flag=0;
        for(int i=n-1;i>0;i--)
        {
         // watch(a[i]);
            if(a[i-1]<a[i])
                flag=1;
        }
        if(flag==1)
            cout<<"no"<<endl;
        else
            cout<<"yes"<<endl;
        cin>>t;
    }
}
